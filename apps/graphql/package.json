{
  "name": "@curio/graphql",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "description": "Generated with `npm create pylon`",
  "scripts": {
    "start": "PYLON_TELEMETRY_DISABLED=1 pylon dev -c \"wrangler dev --port 3000\" --client --client-port 3000 --client-path src/generated/gqty/index.ts",
    "build": "PYLON_TELEMETRY_DISABLED=1 pylon build",
    "deploy": "pnpm build && wrangler deploy",
    "test": "vitest",
    "test:changed": "vitest --changed main",
    "db:local": "turso dev -f .dev/dev.db",
    "db:push": "drizzle-kit push",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:studio": "drizzle-kit studio",
    "generate:openapi": "openapi-typescript openapi/qiita.yaml -o src/generated/openapi/schema.d.ts",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@getcronit/pylon": "^2.0.0",
    "@libsql/client": "^0.15.15",
    "@paralleldrive/cuid2": "^2.2.2",
    "drizzle-orm": "^0.44.6",
    "openapi-fetch": "^0.14.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20251011.0",
    "@getcronit/pylon-dev": "^1.0.0",
    "@vitest/ui": "^3.2.4",
    "drizzle-kit": "^0.31.5",
    "msw": "^2.11.5",
    "openapi-msw": "^1.3.0",
    "openapi-typescript": "^7.9.1",
    "typescript": "5.8.3",
    "vitest": "^3.2.4",
    "wrangler": "^4.43.0"
  }
}
