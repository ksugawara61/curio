{
  "name": "@curio/graphql",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "description": "Generated with `npm create pylon`",
  "scripts": {
    "dev": "PYLON_TELEMETRY_DISABLED=1 pylon dev -c \"wrangler dev --port 3000\" --client --client-port 3000 --client-path src/generated/gqty/index.ts",
    "build": "PYLON_TELEMETRY_DISABLED=1 pylon build",
    "deploy": "pnpm build && wrangler deploy",
    "test": "vitest",
    "test:changed": "vitest --changed main",
    "db:local": "turso dev -f .dev/dev.db",
    "db:push": "drizzle-kit push",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:studio": "drizzle-kit studio",
    "generate:openapi": "openapi-typescript openapi/qiita.yaml -o src/generated/openapi/schema.d.ts",
    "scaffold": "scaffdog generate",
    "scaffold:list": "scaffdog list",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit",
    "codegen": "pnpm build && cp .pylon/schema.graphql ../../packages/graphql-client/src/graphql/schema.graphql"
  },
  "dependencies": {
    "@getcronit/pylon": "catalog:",
    "@libsql/client": "catalog:",
    "@paralleldrive/cuid2": "catalog:",
    "better-auth": "catalog:",
    "drizzle-orm": "catalog:",
    "openapi-fetch": "catalog:",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "catalog:",
    "@curio/eslint-config": "workspace:*",
    "@getcronit/pylon-dev": "catalog:",
    "@types/node": "catalog:",
    "@vitest/ui": "catalog:",
    "drizzle-kit": "catalog:",
    "eslint": "catalog:",
    "msw": "catalog:",
    "openapi-msw": "catalog:",
    "openapi-typescript": "catalog:",
    "scaffdog": "catalog:",
    "typescript": "catalog:",
    "vitest": "catalog:",
    "wrangler": "catalog:"
  }
}
