{
  "name": "@curio/web",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint .",
    "fmt": "eslint --fix .",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "storybook": "storybook dev -p 6006",
    "vrt": "playwright install chromium && playwright test",
    "vrt:dry-run": "playwright install chromium && playwright test --ignore-snapshots",
    "vrt:html": "playwright install chromium && playwright test --reporter=html",
    "vrt:approve": "playwright install chromium && playwright test --update-snapshots all",
    "release": "wrangler pages deploy ./dist --branch main --commit-dirty=true --project-name curio-web"
  },
  "dependencies": {
    "@apollo/client": "catalog:",
    "@clerk/clerk-react": "catalog:",
    "@curio/graphql-client": "workspace:*",
    "@hookform/resolvers": "catalog:",
    "@tanstack/react-router": "^1.120.2",
    "graphql": "catalog:",
    "react": "catalog:",
    "react-dom": "catalog:",
    "react-error-boundary": "catalog:",
    "react-hook-form": "catalog:",
    "react-markdown": "catalog:",
    "remark-gfm": "catalog:",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@curio/eslint-config": "workspace:*",
    "@curio/testing-library": "workspace:*",
    "@playwright/test": "catalog:",
    "@storybook/addon-links": "catalog:",
    "@storybook/addon-themes": "catalog:",
    "@storybook/react": "catalog:",
    "@storybook/react-vite": "catalog:",
    "@tailwindcss/typography": "catalog:",
    "@tailwindcss/vite": "catalog:",
    "@tanstack/router-devtools": "^1.120.2",
    "@tanstack/router-plugin": "^1.120.1",
    "@types/node": "catalog:",
    "@types/react": "catalog:",
    "@types/react-dom": "catalog:",
    "@vitejs/plugin-react": "catalog:",
    "@testing-library/jest-dom": "catalog:",
    "@testing-library/react": "catalog:",
    "@testing-library/user-event": "catalog:",
    "daisyui": "catalog:",
    "eslint": "catalog:",
    "jsdom": "catalog:",
    "eslint-plugin-prefer-arrow-functions": "catalog:",
    "eslint-plugin-react-hooks": "catalog:",
    "msw": "catalog:",
    "msw-storybook-addon": "catalog:",
    "storybook": "catalog:",
    "tailwindcss": "catalog:",
    "typescript": "catalog:",
    "typescript-eslint": "catalog:",
    "vite": "catalog:",
    "vitest": "catalog:",
    "wrangler": "catalog:"
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  }
}
